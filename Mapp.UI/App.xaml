<Application x:Class="Shmap.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Mapp"
             xmlns:viewModels="clr-namespace:Shmap.ViewModels"
             xmlns:visualBasic="clr-namespace:Microsoft.VisualBasic;assembly=Microsoft.VisualBasic.Core"
             xmlns:system="clr-namespace:System;assembly=System.Runtime">
    <Application.Resources>
        <viewModels:ViewModelLocator x:Key="ViewModelLocator" />
        <Style x:Key="FixForSingleApplicationResourceBugInWpf_AtLeastTwoResourcesMustBeDefined" />
        <!--Second entry is fix for WPF issue: https://bengribaudo.com/blog/2010/08/19/106/bug-single-application-resources-entry-ignored-->
    </Application.Resources>
</Application>
